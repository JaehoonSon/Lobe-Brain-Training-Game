{
  "expo": {
    "name": "Lobe",
    "slug": "lobe",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/brain_logo.png",
    "scheme": "lobe",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/brain_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#fe7939"
    },
    "assetBundlePatterns": ["**/*"],
    "locales": {
      "en": "./languages/english.json",
      "es": "./languages/spanish.json",
      "ja": "./languages/japanese.json",
      "zh-Hans": "./languages/chinese-simplified.json",
      "pt-PT": "./languages/portuguese.json",
      "de": "./languages/german.json",
      "fr": "./languages/french.json",
      "hi": "./languages/hindi.json",
      "ru": "./languages/russian.json",
      "ko": "./languages/korean.json",
      "pt-BR": "./languages/portuguese-brazil.json"
    },
    "ios": {
      "appStoreUrl": "https://apps.apple.com/us/app/lobe-brain-training-games/id6757370720",
      "usesAppleSignIn": true,
      "supportsTablet": false,
      "buildNumber": "1",
      "bundleIdentifier": "com.theblucks.brainapp",
      "infoPlist": {
        "CFBundleLocalizations": [
          "en",
          "es",
          "ja",
          "zh-Hans",
          "pt-PT",
          "de",
          "fr",
          "hi",
          "ru",
          "ko",
          "pt-BR"
        ],
        "NSUserTrackingUsageDescription": "We use your data to improve your experience.",
        "CFBundleAllowMixedLocalizations": true,
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.119239050721-dhiahbraqtnql8v44qkvfcmqiud8fac6"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/brain_logo_transparent.png",
        "backgroundColor": "#fa8b4b"
      },
      "package": "com.knuceles.ExpoTemplate"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "expo-video",
        [
          "expo-localization",
          {
            "supportedLocales": {
              "ios": [
                "en",
                "es",
                "ja",
                "zh-Hans",
                "pt-PT",
                "de",
                "fr",
                "hi",
                "ru",
                "ko",
                "pt-BR"
              ],
              "android": [
                "en",
                "es",
                "ja",
                "zh-Hans",
                "pt-PT",
                "de",
                "fr",
                "hi",
                "ru",
                "ko",
                "pt-BR"
              ]
            }
          }
        ],
      "expo-notifications",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 21
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      "expo-web-browser",
      "@react-native-google-signin/google-signin",
      "expo-tracking-transparency",
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "knuceles",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "586a70a9-4ec7-4ec2-a30a-220fd0ebf9d4"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/586a70a9-4ec7-4ec2-a30a-220fd0ebf9d4",
      "checkAutomatically": "ON_LOAD"
    }
  }
}
